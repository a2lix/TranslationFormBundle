{
    "name": "a2lix/translation-form-bundle",
    "type": "symfony-bundle",
    "description": "Translate your doctrine objects easily with some helpers",
    "keywords": [
        "symfony",
        "doctrine2",
        "i18n",
        "internationalization",
        "translation",
        "translatable",
        "form",
        "knplabs",
        "prezent"
    ],
    "homepage": "https://github.com/a2lix/TranslationFormBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David ALLIX",
            "homepage": "http://a2lix.fr"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/a2lix/TranslationFormBundle/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "a2lix/auto-form-bundle": "^0.4",
        "symfony/config": "^6.4|^7.3",
        "symfony/dependency-injection": "^6.4|^7.3",
        "symfony/doctrine-bridge": "^6.4|^7.3",
        "symfony/event-dispatcher": "^6.4|^7.3",
        "symfony/form": "^6.4|^7.3",
        "symfony/http-foundation": "^6.4|^7.3",
        "symfony/http-kernel": "^6.4|^7.3",
        "symfony/options-resolver": "^6.4|^7.3"
    },
    "require-dev": {
        "doctrine/orm": "^3.5",
        "friendsofphp/php-cs-fixer": "^3.45",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.18",
        "rector/rector": "^2.2",
        "symfony/cache": "^6.4|^7.3",
        "symfony/phpunit-bridge": "^6.4|^7.3",
        "symfony/validator": "^6.4|^7.3",
        "vimeo/psalm": "^6.1"
    },
    "suggest": {
        "knplabs/doctrine-behaviors": "For Knp strategy",
        "prezent/doctrine-translatable-bundle": "For Prezent strategy"
    },
    "scripts": {
        "cs-fixer": [
            "php-cs-fixer fix --verbose"
        ],
        "psalm": [
            "psalm"
        ],
        "phpunit": [
            "phpunit"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "A2lix\\TranslationFormBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "A2lix\\TranslationFormBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
